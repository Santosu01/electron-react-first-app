    "allowSyntheticDefaultImports": true,
    "strict": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "outDir": "./electron",
    "baseUrl": "./electron-config",
  },
  "include": ["electron-config/*.ts"],
